{
  "name": "hosthub-multitenant",
  "version": "1.0.0",
  "description": "Multi-tenant booking platform for Airbnb hosts.",
  "main": "server.ts",
  "scripts": {
    "start": "node dist/server.js",
    "build:server": "tsc --project tsconfig.server.json",
    "build:client": "tsc --project tsconfig.client.json",
    "build": "npm run build:server && npm run build:client",
    "dev:server": "nodemon --watch server.ts --exec ts-node server.ts",
    "dev:client": "lite-server --baseDir ./",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "pg": "^8.11.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.12",
    "@types/pg": "^8.11.6",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "concurrently": "^8.2.2",
    "lite-server": "^2.6.1",
    "nodemon": "^3.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}